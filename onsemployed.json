{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://raw.githubusercontent.com/norettevulkan/norettevulkan.github.io/main/data_ONS-mgsc.json",
    "format": {"type": "json", "property": "years"}
  },
  "title": {
    "text": "UK Unemployed (16+)",
    "subtitle": "United Kingdom (thousands) seasonally adjusted",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "width": 300,
  "height": 300,
  "mark": {
    "type": "line",
    "color": "slateblue",
    "strokeWidth": 2,
    "opacity": 1
  },
  "encoding": {
    "x": {"field": "date", "type": "temporal", "title": null},
    "y": {"field": "value", "type": "quantitative", "title": null}
  }
}